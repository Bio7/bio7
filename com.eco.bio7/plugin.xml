   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.eco.bio7.application"
            name="Bio7">
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini"/>
         <property
               name="windowImage"
               value="icons/logo.gif"/>
         <property
               name="startupProgressRect"
               value="95,50,285,30"/>
         <property
               name="aboutImage"
               value="icons/bio7app/bio7_48.png"/>
         <property
               name="startupForegroundColor"
               value="000000"/>
         <property
               name="startupMessageRect"
               value="0,0,0,0"/>
         <property
               name="appName"
               value="Bio7">
         </property>
         <property
               name="aboutText"
               value="Bio7 - An Integrated Development Environment for Ecological Modeling&#x0A;&#x0A;Developer: M. Austenfeld&#x0A;&#x0A;For more information about Bio7 visit the website:&#x0A;&#x0A;http://bio7.org&#x0A;&#x0A;-----------------------------------------------------------">
         </property>
         <property
               name="windowImages"
               value="icons/bio7app/bio7_16.png,icons/bio7app/bio7_32.png,icons/bio7app/bio7_48.png,icons/bio7app/bio7_64.png,icons/bio7app/bio7_128.png">
         </property>
         
         <property
	name="applicationCSSResources"
	value="platform:/plugin/com.eco.bio7.themes/images/">
</property>
      </product>
   </extension>
